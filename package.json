{
  "name": "cinemyar",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A production-ready React template with TypeScript, Vite, TailwindCSS, and comprehensive component library",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "tailwindcss",
    "template",
    "component-library"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "generate:env": "tsx scripts/generate-env.ts"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@reduxjs/toolkit": "^2.9.2",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/react-query": "^5.90.5",
    "@types/video.js": "^7.3.58",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.545.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.4",
    "redux": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "video.js": "^8.23.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.38.0",
    "@storybook/addon-a11y": "^9.1.15",
    "@storybook/addon-docs": "^9.1.15",
    "@storybook/addon-onboarding": "^9.1.15",
    "@storybook/addon-vitest": "^9.1.15",
    "@storybook/react-vite": "^9.1.15",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dotenv": "^8.2.3",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-helmet-async": "^1.0.3",
    "@vitejs/plugin-react-swc": "^4.2.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^9.1.15",
    "globals": "^16.4.0",
    "handlebars": "^4.7.8",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "json-server": "1.0.0-beta.3",
    "playwright": "^1.56.1",
    "plop": "^4.0.4",
    "postcss": "^8.5.6",
    "storybook": "^9.1.15",
    "tailwindcss": "^4.1.16",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vitest": "^3.2.4"
  }
}
